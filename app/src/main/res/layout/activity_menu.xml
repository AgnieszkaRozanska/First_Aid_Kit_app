<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".Menu"
android:background="@drawable/menu">

    <ImageView
            android:layout_width="430dp"
            android:layout_height="161dp"
            app:srcCompat="@drawable/logo"
            android:id="@+id/imageView_logo2"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
    />
    <Button
            android:background="@drawable/ateczkaprzycisk"
            android:layout_width="119dp"
            android:layout_height="141dp"
            android:id="@+id/Button_FirstAidKit"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="248dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="52dp"/>

    <Button android:id="@+id/Button_YourAllMedicines"
            android:background="@drawable/twojelekiprzycisk"
            android:layout_width="114dp"
            android:layout_height="141dp"
            app:layout_constraintStart_toEndOf="@+id/Button_FirstAidKit"
            android:layout_marginStart="56dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="64dp"
            android:layout_marginTop="248dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.567"/>



</android.support.constraint.ConstraintLayout>