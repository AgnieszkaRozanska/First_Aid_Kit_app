<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        xmlns:tools="http://schemas.android.com/tools"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:background="@drawable/twojeleki"
        tools:context=".takeMedicineOccur.ActivityMedicinesMenu">


    <Button
            android:id="@+id/Button_All_Med_Drugs"
            android:layout_width="135dp"
            android:layout_height="180dp"
            android:layout_marginStart="56dp"
            android:layout_marginTop="176dp"
            android:layout_marginEnd="64dp"
            android:background="@drawable/wszystkielekiprzycisk"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.423"
            app:layout_constraintStart_toEndOf="@+id/Button_AddTakeMedOccur_addMedToTake"
            app:layout_constraintTop_toTopOf="parent" />

    <Button
            android:id="@+id/Button_AddTakeMedOccur_addMedToTake"
            android:layout_width="120dp"
            android:layout_height="180dp"
            android:layout_marginStart="45dp"
            android:layout_marginTop="176dp"
            android:background="@drawable/dodajlekprzyisk"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent" />

    <Button
            android:id="@+id/Button_ShowAllMedicinesToday"
            android:layout_width="156dp"
            android:layout_height="209dp"
            android:layout_marginBottom="40dp"
            android:gravity="center_vertical"
            android:background="@drawable/buttonmedtoday"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/Button_AddTakeMedOccur_addMedToTake" />

</android.support.constraint.ConstraintLayout>