<?xml version="1.0" encoding="utf-8"?>
<android.support.constraint.ConstraintLayout
        xmlns:android="http://schemas.android.com/apk/res/android"
        xmlns:tools="http://schemas.android.com/tools"
        xmlns:app="http://schemas.android.com/apk/res-auto"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        tools:context=".takeMedicineOccur.ActivityMedicinesMenu"
android:background="@drawable/twojeleki">


    <Button android:id="@+id/Button_All_Med_Drugs"
            android:background="@drawable/wszystkielekiprzycisk"
            android:layout_width="135dp"
            android:layout_height="180dp"
            app:layout_constraintStart_toEndOf="@+id/Button_AddTakeMedOccur_addMedToTake"
            android:layout_marginStart="56dp"
            app:layout_constraintEnd_toEndOf="parent"
            android:layout_marginEnd="64dp"
            android:layout_marginTop="176dp"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintHorizontal_bias="0.423"/>
    <Button
            android:background="@drawable/dodajlekprzyisk"
            android:layout_width="120dp"
            android:layout_height="180dp"
            android:id="@+id/Button_AddTakeMedOccur_addMedToTake"
            app:layout_constraintStart_toStartOf="parent"
            android:layout_marginTop="176dp"
            app:layout_constraintTop_toTopOf="parent"
            android:layout_marginStart="60dp"/>
    <Button
            android:layout_width="156dp"
            android:layout_height="209dp"
            android:background="@drawable/buttonmedtoday"
            android:id="@+id/Button_ShowAllMedicinesToday"
            android:layout_marginBottom="8dp"
            app:layout_constraintBottom_toBottomOf="parent" app:layout_constraintStart_toStartOf="parent"
            android:layout_marginStart="8dp" app:layout_constraintEnd_toEndOf="parent" android:layout_marginEnd="8dp"
            android:layout_marginTop="8dp" app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.754"/>

</android.support.constraint.ConstraintLayout>